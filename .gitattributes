# Git LFS (Large File Storage) configuration
# Track large data files
*.csv filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
*.joblib filter=lfs diff=lfs merge=lfs -text
*.pkl filter=lfs diff=lfs merge=lfs -text
*.pickle filter=lfs diff=lfs merge=lfs -text
*.h5 filter=lfs diff=lfs merge=lfs -text
*.hdf5 filter=lfs diff=lfs merge=lfs -text
*.parquet filter=lfs diff=lfs merge=lfs -text
*.feather filter=lfs diff=lfs merge=lfs -text

# Track large image files
*.jpg filter=lfs diff=lfs merge=lfs -text
*.jpeg filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
*.gif filter=lfs diff=lfs merge=lfs -text
*.bmp filter=lfs diff=lfs merge=lfs -text
*.tiff filter=lfs diff=lfs merge=lfs -text
*.tif filter=lfs diff=lfs merge=lfs -text

# Track model files
*.pt filter=lfs diff=lfs merge=lfs -text
*.pth filter=lfs diff=lfs merge=lfs -text
*.onnx filter=lfs diff=lfs merge=lfs -text
*.pb filter=lfs diff=lfs merge=lfs -text
*.savedmodel filter=lfs diff=lfs merge=lfs -text

# Track audio/video files
*.mp3 filter=lfs diff=lfs merge=lfs -text
*.mp4 filter=lfs diff=lfs merge=lfs -text
*.avi filter=lfs diff=lfs merge=lfs -text
*.mov filter=lfs diff=lfs merge=lfs -text
*.wav filter=lfs diff=lfs merge=lfs -text

# Track document files
*.pdf filter=lfs diff=lfs merge=lfs -text
*.doc filter=lfs diff=lfs merge=lfs -text
*.docx filter=lfs diff=lfs merge=lfs -text
*.xls filter=lfs diff=lfs merge=lfs -text
*.xlsx filter=lfs diff=lfs merge=lfs -text

# Track archive files
*.tar filter=lfs diff=lfs merge=lfs -text
*.gz filter=lfs diff=lfs merge=lfs -text
*.bz2 filter=lfs diff=lfs merge=lfs -text
*.7z filter=lfs diff=lfs merge=lfs -text
*.rar filter=lfs diff=lfs merge=lfs -text

# Track specific large files in this project
csv/train.csv filter=lfs diff=lfs merge=lfs -text
models/stacking_model.joblib filter=lfs diff=lfs merge=lfs -text
models/xgb_model.joblib filter=lfs diff=lfs merge=lfs -text
nfl-big-data-bowl-2020.zip filter=lfs diff=lfs merge=lfs -text
01nfl-integrity-bcfl-videoSixteenByNine3000.jpg filter=lfs diff=lfs merge=lfs -text
Screenshot 2025-07-27 at 12.04.10 PM.png filter=lfs diff=lfs merge=lfs -text
static/img/navbar-logo.png filter=lfs diff=lfs merge=lfs -text
static/img/nfl-hero-image.jpg filter=lfs diff=lfs merge=lfs -text

# Track any files larger than 50MB
*.{csv,zip,joblib,pkl,pickle,h5,hdf5,parquet,feather,jpg,jpeg,png,gif,bmp,tiff,tif,pt,pth,onnx,pb,mp3,mp4,avi,mov,wav,pdf,doc,docx,xls,xlsx,tar,gz,bz2,7z,rar} filter=lfs diff=lfs merge=lfs -text 